{
    "name": "mobilecosystem/template-1",
    "description": "MobileCosystem Skeleton Template #1",
    "version": "0.1.2",
    "type": "project",
    "keywords": ["laravel", "mobilecosystem", "php", "projects", "walisph"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jay Are Galinada",
            "email": "jayaregalinada@gmail.com"
        },
        {
            "name": "Mark Donguines",
            "role": "Graphic Designer"
        },
        {
            "name": "Jun Sabijon",
            "role": "Team Leader"
        }
    ],
    "support": {
        "issues": "https://github.com/mobilecosystem/template-1"
    },
    "require": {
        "laravel/framework": "4.2.*",
        "cartalyst/sentry": "2.0.*",
        "vtalbot/markdown": "1.5"
    },
    "require-dev": {
        "way/generators": "dev-master",
        "orangehill/iseed": "dev-master",
        "phpunit/phpunit": "3.7.*",
        "mockery/mockery": "dev-master",
        "itsgoingd/clockwork": "1.*"
    },
    "autoload": {
        "classmap": [
            "app/commands",
            "app/controllers",
            "app/models",
            "app/database/migrations",
            "app/database/seeds",
            "app/tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable"
}
